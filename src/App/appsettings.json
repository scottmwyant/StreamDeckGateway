{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Warning",
      "App.Worker": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": true,
        "SingleLine": true,
        "ColorBehavior": "Enabled"
      }
    }
  },
  "StreamDeckGateway": {
    "readTimeoutSeconds": 300
  }
}
